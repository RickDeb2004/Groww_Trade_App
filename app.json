    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/stock-market.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.expostocksapp"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "ALPHA_VANTAGE_KEY": " JN8P7QY4PX6UQLFW",
      "router": {},
      "eas": {
        "projectId": "5e003def-0413-41dc-ad60-2c126b48ad4f"
      }
    }
  }
}

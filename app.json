{
  "expo": {
    "name": "Expo Stocks App",
    "slug": "expo-stocks-app",
    "scheme": "stocksapp",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/stock-market.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/stock-market.png",
